        "attributes": {
          "name": "Buy confidence",
          "properties": {
            "shape": "chronograf-v2",
            "type": "gauge",
            "queries": [
              {
                "text": "data = from(bucket: \"primary\")\n  |> range(start: -1w)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"recommendation_trend\" and r.symbol == v.symbol)\n  |> filter(fn: (r) => r._field != \"period\")\n  |> last()\n\ntotal = data\n  |> group()\n  |> sum()\n  |> findColumn(column: \"_value\", fn: (key) => true)\n\ndata\n  |> map(fn: (r) => {\n    x = if r._field == \"strongSell\" then\n      0.0 / 4.0 * float(v: r._value)\n    else if r._field == \"sell\" then\n      1.0 / 4.0 * float(v: r._value)\n    else if r._field == \"buy\" then\n      3.0 / 4.0 * float(v: r._value)\n    else if r._field == \"strongBuy\" then\n      4.0 / 4.0 * float(v: r._value)\n    else\n      2.0 / 4.0 * float(v: r._value)\n    return { _value: 100.0 * x / float(v: total[0])  }\n  })\n  |> sum()\n",
